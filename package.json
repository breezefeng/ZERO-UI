      "eslint --fix"
    ],
    "components/**/**/*.less": [
      "stylelint --syntax less --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "classnames": "^2.3.1",
